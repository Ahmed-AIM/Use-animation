{
  "name": "react-useanimations",
  "version": "1.2.12",
  "description": "Official React component for useanimations icons",
  "repository": {
    "type": "git",
    "url": "https://github.com/useAnimations/react-useanimations.git"
  },
  "main": "dist/index",
  "author": "Tuan Phung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/useAnimations/react-useanimations/issues"
  },
  "homepage": "https://github.com/useAnimations/react-useanimations",
  "keywords": [
    "microanimations",
    "small",
    "svg",
    "icons",
    "design",
    "useanimations"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src -d es2015 --ignore '__tests__','**/*.d.ts' --extensions '.ts,.tsx' --out-dir ./dist",
    "postbuild": "yarn build:types",
    "build:watch": "yarn build --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "check-types": "tsc --noEmit",
    "check-types:watch": "yarn type-check -- --watch"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "lottie-web": "^5.5.7"
  },
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.6.2",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "nodemon": "^1.19.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
