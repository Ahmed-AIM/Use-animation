{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "arrow-parens": "off",
    "react/jsx-props-no-spreading": "off",
    "react/prop-types": "off",
    "rest-spread-spacing": "off",
    "object-curly-newline": "off"
  }
}